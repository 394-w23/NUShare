{
  "name": "vite-template-react",
  "version": "1.2.0",
  "scripts": {
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "cy:open": "cypress open --e2e",
    "em:start": "firebase emulators:start --import=./saved-data --export-on-exit=./saved-data",
    "em:run": "firebase emulators:exec --import=./saved-data 'npm start'",
    "em:exec": "firebase emulators:exec --import=./saved-data 'npm start'",
    "em:execui": "firebase emulators:exec --ui --import=./saved-data 'npm start'"
  },
  "dependencies": {
    "bootstrap": "^5.2.3",
    "bootstrap-icons": "^1.10.3",
    "firebase": "^9.15.0",
    "moment": "^2.29.4",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-router-dom": "^6.6.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "^0.25.8",
    "@vitest/ui": "^0.25.8",
    "c8": "^7.12.0",
    "cypress": "^12.4.1",
    "jest-when": "^3.5.2",
    "jsdom": "^20.0.3",
    "vite": "^4.0.0"
  }
}
